{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "start",
			"command": "npm start",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"isBackground": true,			
			"problemMatcher": {
				"base": "$tsc-watch",
				"background": {
					"activeOnStart": true,
					"beginsPattern": "react-scripts start",
					"endsPattern": "Compiled successfully!"
				}
			}
		}
	]
}